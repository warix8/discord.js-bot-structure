{
  "name": "discord.js-bot-structure",
  "version": "1.0.0",
  "description": "An basic discord.js-bot-structure",
  "main": "main.ts",
  "scripts": {
    "test": "tsc --noEmit",
    "compile": "tsc",
    "watch": "tsc --watch",
    "start": "tsc && node dist/shards.js",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --check **/*.ts",
    "check": "yarn lint && yarn prettier && yarn test",
    "format": "prettier --write **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/warix8/discord.js-bot-structure.git"
  },
  "author": "Warix",
